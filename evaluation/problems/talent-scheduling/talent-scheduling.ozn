output ["penSum = "++format(penSum)++"\n","cost = ",show(cost),";\ns = ",show(s),";\nslot = ",show(slot),";\nfirstSlot = ",show(firstSlot),";\nlastSlot = ",show(lastSlot),";\ncumDur = ",show(cumDur),";\nfirstJob = ",show(firstJob),";\nlastJob = ",show(lastJob),";\nwait = ",show(wait),";\na = ",show(a),";\n"];
set of int: Actors = 1..4;
set of int: Scenes = 1..6;
array [Scenes] of set of int: a = [1..2,2..3,3..4,{2,4},{1,3},{1,4}];
array [Scenes] of int: d = [1,2,3,4,5,6];
array [Scenes] of int: s;
array [Scenes] of int: slot;
array [Actors] of int: firstSlot;
array [Actors] of int: lastSlot;
array [Actors] of int: wait;
set of int: ScenesExt = 1..7;
array [ScenesExt] of int: cumDur = [sum([d[s[s_]] | s_ in 1..sc-1, ]) | sc in ScenesExt, ];
array [Actors] of int: firstJob = [cumDur[firstSlot[a]] | a in Actors, ];
array [Actors] of int: lastJob = [cumDur[lastSlot[a]+1] | a in Actors, ];
int: cost;
int: penSum;
